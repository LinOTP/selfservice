{
  "name": "self-service",
  "version": "0.2.0",
  "license": "GPL-3.0-only",
  "scripts": {
    "ng": "ng",
    "start": "yarn start-en",
    "start-de": "yarn start-base --configuration=de",
    "start-en": "yarn start-base --configuration=en",
    "start-base": "ng serve --proxy-config .dev-proxy/proxy.conf.js",
    "start:ki-cloud": "ng serve --ssl --proxy-config .dev-proxy/ki-cloud.conf.js",
    "build": "urlprefix=\"\" yarn build-with-prefix",
    "build-with-prefix": "for lang in de en; do ng build --configuration=production-$lang --base-href=${urlprefix}/$lang/; done",
    "test": "yarn test:chrome",
    "test:chrome": "ng test --browsers Chrome",
    "test:chromium": "ng test --browsers Chromium",
    "test:firefox": "ng test --browsers Firefox",
    "test:all": "ng test --watch=false",
    "test:ci": "ng test --browsers ChromeHeadless --watch=false --code-coverage",
    "test:ci-no-sandbox": "ng test --browsers ChromeHeadlessNoSandbox --code-coverage --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract-i18n": "ng xi18n self-service --i18n-format xlf --output-path locale --i18n-locale en && ngx-extractor -i \"./src/**/*.ts\" -o ./src/locale/messages.xlf",
    "postextract-i18n": "ng run self-service:xliffmerge"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.4",
    "@angular/cdk": "^8.1.1",
    "@angular/common": "^8.2.4",
    "@angular/compiler": "^8.2.4",
    "@angular/core": "^8.2.4",
    "@angular/forms": "^8.2.4",
    "@angular/material": "^8.1.1",
    "@angular/platform-browser": "^8.2.4",
    "@angular/platform-browser-dynamic": "^8.2.4",
    "@angular/router": "^8.2.4",
    "@ngx-i18nsupport/tooling": "8.0.3",
    "@ngx-translate/i18n-polyfill": "^1.0.0",
    "angularx-qrcode": "^1.6.2",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "ng2-validation": "^4.2.0",
    "ngx-cookie": "^2.0.1",
    "ngx-permissions": "^7.0.3",
    "rxjs": "^6.5.3",
    "tslib": "^1.9.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.2",
    "@angular/cli": "^8.3.2",
    "@angular/compiler-cli": "^8.2.4",
    "@angular/language-service": "^8.2.4",
    "@ngx-i18nsupport/ngx-i18nsupport": "^1.1.3",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "tslint-jasmine-rules": "^1.3.2",
    "typescript": "~3.5.3"
  }
}